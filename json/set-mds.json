{
  "command": "set-mds",
  "description": "Edit existing object of type Multi-Domain Server or Multi-Domain Log Server using object name or uid.",
  "request": {
    "url": "POST https://<mgmt-server>:<port>/web_api/set-mds",
    "headers": [
      {
        "name": "Content-Type",
        "value": "application/json",
        "description": "Send JSON object to use the API Web Services"
      },
      {
        "name": "X-chkp-sid",
        "value": "string token",
        "description": "Session unique identifier as it returned by the login request"
      }
    ],
    "body": [
      {
        "name": "uid",
        "description": "Object unique identifier.",
        "type": "string",
        "required": true
      },
      {
        "name": "ipv6-address",
        "description": "IPv6 Address. requires manual restart.",
        "type": "string",
        "required": false
      },
      {
        "name": "hardware",
        "description": "Hardware name. For example: Open server, Smart-1, Other.",
        "type": "string",
        "required": false
      },
      {
        "name": "new-name",
        "description": "New name of the object.",
        "type": "string",
        "required": false
      },
      {
        "name": "os",
        "description": "Operating system name. For example: Gaia, Linux, SecurePlatform.",
        "type": "string",
        "required": false
      },
      {
        "name": "version",
        "description": "System version.",
        "type": "string",
        "required": false
      },
      {
        "name": "one-time-password",
        "description": "Secure internal connection one time password.",
        "type": "string",
        "required": false
      },
      {
        "name": "ip-pool-first",
        "description": "First IP address in the range.",
        "type": "string",
        "required": false
      },
      {
        "name": "ip-pool-last",
        "description": "Last IP address in the range.",
        "type": "string",
        "required": false
      }
    ]
  },
