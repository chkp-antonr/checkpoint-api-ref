{
  "command": "add-tacacs-server",
  "description": "Create new object.",
  "request": {
    "url": "POST https://<mgmt-server>:<port>/web_api/add-tacacs-server",
    "headers": [
      {
        "name": "Content-Type",
        "value": "application/json",
        "description": "Send JSON object to use the API Web Services"
      },
      {
        "name": "X-chkp-sid",
        "value": "string token",
        "description": "Session unique identifier as it returned by the login request"
      }
    ],
    "body": [
      {
        "name": "name",
        "description": "Object name. Must be unique in the domain.",
        "type": "string",
        "required": true
      },
      {
        "name": "secret-key",
        "description": "The server's secret key.Required only when \"server-type\" was selected to be \"TACACS+\".",
        "type": "string",
        "required": true
      },
      {
        "name": "server",
        "description": "The UID or Name of the host that is the TACACS Server.",
        "type": "string",
        "required": true
      },
      {
        "name": "encryption",
        "description": "Is there a secret key defined on the server. Must be set true when \"server-type\" was selected to be \"TACACS+\".",
        "type": "boolean",
        "required": false,
        "default": "false"
      },
      {
        "name": "priority",
        "description": "The priority of the TACACS Server in case it is a member of a TACACS Group.",
        "type": "integer",
        "required": false,
        "default": "1"
      },
      {
        "name": "server-type",
        "description": "Server type, TACACS or TACACS+.",
        "type": "string",
        "required": false,
        "default": "TACACS",
        "valid_values": [
          "TACACS",
          "TACACS+"
        ]
      },
      {
        "name": "service",
        "description": "Server service, only relevant when \"server-type\" is TACACS.",
        "type": "string",
        "required": false,
        "default": "TACACS"
      }
    ]
  },
