{
  "command": "clone-domain",
  "description": "Clones an existing Domain and applicable Check Point configuration. This command applies only to a Multi-Domain Security Management Server. This command is available only after you log in to the System Data domain. For more information and list of limitations, see sk180631.",
  "request": {
    "url": "POST https://<mgmt-server>:<port>/web_api/clone-domain",
    "headers": [
      {
        "name": "Content-Type",
        "value": "application/json",
        "description": "Send JSON object to use the API Web Services"
      },
      {
        "name": "X-chkp-sid",
        "value": "string token",
        "description": "Session unique identifier as it returned by the login request"
      }
    ],
    "body": [
      {
        "name": "name",
        "description": "Name of the source Domain to clone.",
        "type": "string",
        "required": true
      },
      {
        "name": "new-domain-name",
        "description": "Name of the new Domain.",
        "type": "string",
        "required": true
      },
      {
        "name": "new-domain-server-name",
        "description": "Name of the new Domain Server.",
        "type": "string",
        "required": true
      },
      {
        "name": "new-domain-server-ip",
        "description": "IPv4 Address of the new Domain Server.",
        "type": "string",
        "required": true
      },
      {
        "name": "new-domain-server-ipv6",
        "description": "IPv6 Address of the new Domain Server.Required only if the source Domain has an IPv6 address.",
        "type": "string",
        "required": true
      },
      {
        "name": "omit-sensitive-info",
        "description": "Remove sensitive information from exported database.",
        "type": "boolean",
        "required": false,
        "default": "false"
      }
