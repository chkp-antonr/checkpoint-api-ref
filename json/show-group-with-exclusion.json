  "examples": {
    "show-group-with-exclusion": {
      "description": "",
      "request": "POST {{server}}/show-group-with-exclusion\nContent-Type: application/json\nX-chkp-sid: {{session}}\n\n{\n  \"name\" : \"Group with exclusion\"\n}",
      "response": ""
    },
    "show-group-with-exclusion as ranges": {
