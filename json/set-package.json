{
  "command": "set-package",
  "description": "Edit existing object using object name or uid.",
  "request": {
    "url": "POST https://<mgmt-server>:<port>/web_api/set-package",
    "headers": [
      {
        "name": "Content-Type",
        "value": "application/json",
        "description": "Send JSON object to use the API Web Services"
      },
      {
        "name": "X-chkp-sid",
        "value": "string token",
        "description": "Session unique identifier as it returned by the login request"
      }
    ],
    "body": [
      {
        "name": "uid",
        "description": "Object unique identifier.",
        "type": "string",
        "required": true
      },
      {
        "name": "access",
        "description": "True - enables, False - disables access & NAT policies, empty - nothing is changed.",
        "type": "boolean",
        "required": false
      },
      {
        "name": "access-layers",
        "description": "",
        "type": "array",
        "required": false
      },
      {
        "name": "add",
        "description": "",
        "type": "array",
        "required": false
      },
      {
        "name": "name",
        "description": "Layer name or UID.",
        "type": "string",
        "required": true
      },
      {
        "name": "position",
        "description": "Layer position.",
        "type": "integer",
        "required": true
      },
      {
        "name": "remove",
        "description": "Collection of Access layer objects to be removed identified by the name or UID.",
        "type": "array",
        "required": false
      },
      {
        "name": "value",
        "description": "Collection of Access layer objects to be set identified by the name or UID. Replaces existing Access layers.",
        "type": "array",
        "required": false
      },
      {
