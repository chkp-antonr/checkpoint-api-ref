{
  "command": "add-radius-server",
  "description": "Create new object.",
  "request": {
    "url": "POST https://<mgmt-server>:<port>/web_api/add-radius-server",
    "headers": [
      {
        "name": "Content-Type",
        "value": "application/json",
        "description": "Send JSON object to use the API Web Services"
      },
      {
        "name": "X-chkp-sid",
        "value": "string token",
        "description": "Session unique identifier as it returned by the login request"
      }
    ],
    "body": [
      {
        "name": "name",
        "description": "Object name. Must be unique in the domain.",
        "type": "string",
        "required": true
      },
      {
        "name": "server",
        "description": "The UID or Name of the host that is the RADIUS Server.",
        "type": "string",
        "required": true
      },
      {
        "name": "shared-secret",
        "description": "The secret between the RADIUS server and the Security Gateway.",
        "type": "string",
        "required": true
      },
      {
        "name": "service",
        "description": "The UID or Name of the Service to which the RADIUS server listens.",
        "type": "string",
        "required": false,
        "default": "RADIUS"
      },
      {
        "name": "version",
        "description": "The version can be either RADIUS Version 1.0, which is RFC 2138 compliant, and RADIUS Version 2.0 which is RFC 2865 compliant.",
        "type": "string",
        "required": false,
        "default": "RADIUS Ver. 1.0",
        "valid_values": [
          "RADIUS Ver. 1.0",
          "RADIUS Ver. 2.0"
        ]
      },
      {
        "name": "protocol",
        "description": "The type of authentication protocol that will be used when authenticating the user to the RADIUS server.",
        "type": "string",
        "required": false,
        "default": "PAP",
        "valid_values": [
          "PAP",
          "MS_CHAP2"
        ]
      },
      {
        "name": "priority",
        "description": "The priority of the RADIUS Server in case it is a member of a RADIUS Group.",
        "type": "integer",
        "required": false,
        "default": "1",
        "valid_values": [
          "1-100"
        ]
      },
      {
        "name": "accounting",
        "description": "",
        "type": "Object",
        "required": false,
        "default": "false"
      },
      {
        "name": "enable-ip-pool-management",
        "description": "IP pool management, enables Accounting service.",
        "type": "boolean",
        "required": false,
        "default": "false"
      },
      {
        "name": "accounting-service",
        "description": "The UID or Name of the accounting interface to notify the server when users login and logout which will then lock and release the IP addresses that the server allocated to those users.",
        "type": "string",
        "required": false,
        "default": "RADIUS-ACCOUNTING"
      }
    ]
  },
